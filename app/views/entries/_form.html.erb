<%= render "layouts/errors", :object => @entry %>

<%=form_for(@entry, html: {class: "form-group"}) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title, class: "form-control" %>
  <%= f.label :body %>
  <br>
  <%= f.text_area(:body, size: "60x12") %>
  <br>
  <%= f.submit "Store", class: "btn btn-primary"  %>
<% end %>
